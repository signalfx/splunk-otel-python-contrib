# Cisco CircuIT credentials (used to mint LiteLLM upstream tokens)
CISCO_APP_KEY=
CISCO_CLIENT_ID=
CISCO_CLIENT_SECRET=
CISCO_TOKEN_URL=https://id.cisco.com/oauth2/default/v1/token
CIRCUIT_TOKEN_CACHE=/tmp/circuit_shim_token.json
CIRCUIT_UPSTREAM_BASE=https://chat-ai.cisco.com
CIRCUIT_API_BASE=http://circuit-shim:5001/
CIRCUIT_SHIM_PORT=5001
CIRCUIT_SHIM_API_KEY=shim-local-token

# LiteLLM container
LITELLM_PORT=4000

# LiteLLM proxy settings
LITELLM_BASE_URL=http://localhost:4000/v1
LITELLM_API_KEY=litellm-demo-key
LITELLM_MODEL=gpt-4o
LITELLM_TEMPERATURE=0.1
LITELLM_TIMEOUT=60
LITELLM_MAX_RETRIES=2
LITELLM_PROXY_KEY=litellm-demo-key
LITELLM_MODEL_NAME=gpt-4o
LITELLM_UPSTREAM_MODEL=gpt-4o
LITELLM_SMOKE_PROMPT="Say hello and confirm you reached me via LiteLLM."

# Optional default user prompt
TRAVEL_DEMO_PROMPT="book a flight from BOS to JFK and a stay at McKittrick Hotel"
