# OAuth2 Credentials (Cisco Chat AI in this example)
OAUTH2_CLIENT_ID=your-client-id
OAUTH2_CLIENT_SECRET=your-client-secret
OAUTH2_APP_KEY=your-app-key

# Optional: Override defaults
# OAUTH2_TOKEN_URL=https://id.cisco.com/oauth2/default/v1/token
# OAUTH2_LLM_BASE_URL=https://chat-ai.cisco.com/openai/deployments

# OpenTelemetry Configuration
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317
OTEL_CONSOLE_OUTPUT=true

# CrewAI Configuration
CREWAI_DISABLE_TELEMETRY=true
OTEL_INSTRUMENTATION_GENAI_EMITTERS=span_metric
